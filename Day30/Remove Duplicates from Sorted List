# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def deleteDuplicates(self, head: Optional[ListNode]) -> Optional[ListNode]:
        if not head:
            return None
        if not head.next:
            return head
        prev=head
        current=head.next
        while current:
            if current.val==prev.val:
                prev.next=current.next
                current=current.next
            else:
                prev=current
                current=current.next
        return head
