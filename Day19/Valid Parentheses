class Solution(object):
    def isValid(self, s):
        stack=[]
        for n in s:
            if n==")":
                if stack and stack[-1]=='(':
                    stack.pop()
                else:
                    return False
            elif n=='}':
                if stack and stack[-1]=='{':
                    stack.pop()
                else:
                    return False
            elif n==']':
                if stack and stack[-1]=='[':
                    stack.pop()
                else:
                    return False
            else:
                stack.append(n)
        if stack: return False
        else: return True
