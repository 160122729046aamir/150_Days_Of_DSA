class Solution(object):
    def subarraySum(self, nums, k):
        result=0
        prefixsum={0:1}
        cur_sum=0
        for i in nums:
            cur_sum+=i
            diff=cur_sum-k
            if diff in prefixsum:
                result+=prefixsum[diff]
            if cur_sum in prefixsum:
                prefixsum[cur_sum]+=1
            else:
                prefixsum[cur_sum]=1
        return result







