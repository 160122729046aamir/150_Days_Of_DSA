# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def partition(self, head: Optional[ListNode], x: int):
        if not head or not head.next:
            return head
        less=[]
        greater=[]
        current=head
        while current:
            if current.val<x:
                less.append(current.val)
            elif current.val>=x:
                greater.append(current.val)
            current=current.next
        dummy=ListNode()
        tail=dummy
        for i in less:
            tail.next=ListNode(i)
            tail=tail.next
        for i in greater:
            tail.next=ListNode(i)
            tail=tail.next
        return dummy.next
