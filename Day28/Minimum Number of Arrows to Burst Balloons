class Solution:
    def findMinArrowShots(self, points: List[List[int]]) -> int:
        points.sort()
        output=[points[0]]
        count=len(points)
        for start,end in points[1:]:
            if start<=output[-1][1]:
                output[-1][1]=min(output[-1][1],end)
                count-=1
            else:
                output.append([start,end])
        return count
