class Solution(object):
    def containsNearbyDuplicate(self, nums, k):
        hashmap={}
        for i,n in enumerate(nums):
            if n not in hashmap:
                hashmap[n]=i
            else:
                if abs(hashmap[n]-i)<=k:
                    return True
                else:
                    hashmap[n]=i
        return False

        
