from collections import Counter
class Solution:
    def transform(self, A, B): 
        p1,p2=len(A)-1,len(B)-1
        count=0
        a=Counter(A)
        b=Counter(B)
        if a!=b:
            return -1
        while p1>=0 and p2>=0:
            if A[p1]!=B[p2]:
                count+=1
                p1-=1
            else:
                p1-=1
                p2-=1
                
        return count
